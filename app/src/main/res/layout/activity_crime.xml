<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/searchView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.capstone.safebeacon.MapsActivity">

    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        tools:context="com.daniel.safebeacon.MapsActivity" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="65dp"
            android:layout_alignParentTop="true">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:src="@drawable/searchbox"/>

            <EditText
                android:id="@+id/input_search"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:background="@null"
                android:hint=" Enter Address, City, or Zipcode"
                android:imeOptions="actionSearch"
                android:inputType="textEmailAddress"
                android:textColor="#000"
                android:textSize="20sp"
                tools:ignore="HardcodedText" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="65dp"
            android:layout_alignParentBottom="true">

            <ImageView
                android:id="@+id/toolbarBG2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:scaleType="fitXY"
                android:src="@drawable/toolbar2" />

            <RelativeLayout
                android:layout_width="95dp"
                android:layout_height="65dp"
                android:layout_alignParentEnd="true">
                <ImageView
                    android:id="@+id/buttonBG3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:scaleType="fitXY"
                    android:src="@drawable/buttongrey" />

                <ImageButton
                    android:id="@+id/carIcon"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"
                    android:layout_width="70dp"
                    android:layout_height="35dp"
                    android:background="@null"
                    android:scaleType="fitXY"
                    android:src="@drawable/caricon" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="95dp"
                android:layout_height="65dp">
                <ImageView
                    android:id="@+id/buttonBG4"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:scaleType="fitXY"
                    android:src="@drawable/buttongrey" />

                <ImageButton
                    android:id="@+id/accountIcon2"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@null"
                    android:scaleType="fitXY"
                    android:src="@drawable/accounticon2" />
            </RelativeLayout>

        </RelativeLayout>

        <ImageView
            android:id="@+id/circleBG1"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_width="80dp"
            android:layout_height="79dp"
            android:background="@null"
            android:scaleType="fitXY"
            android:src="@drawable/circlecamera" />

        <ImageButton
            android:id="@+id/searchButton"
            android:layout_width="76dp"
            android:layout_height="69dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="5dp"
            android:background="@null"
            android:scaleType="centerInside"
            android:src="@drawable/searchicon"/>
    </RelativeLayout>
    <!--<ImageButton
        android:id="@+id/locationButton"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="120dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:background="#fff"
        android:src="@drawable/ic_location"
        android:onClick="setMyLocation"/>

    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginLeft="55dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/white_border"
        android:elevation="10dp"
        android:visibility="invisible">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="15dp"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:src="@drawable/ic_magnify"
            tools:ignore="ContentDescription" />

        <EditText
            android:id="@+id/input_search"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:background="@null"
            android:hint=" Enter Address, City, or Zipcode"
            android:imeOptions="actionSearch"
            android:inputType="textEmailAddress"
            android:textColor="#000"
            android:textSize="15sp"
            tools:ignore="HardcodedText" />
    </RelativeLayout>

    <Switch
        android:id="@+id/switchView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginTop="17dp"
        android:layout_marginRight="10dp"
        android:checked="false"
        android:theme="@style/SwitchCompatTheme"
        tools:checked="false" />-->

</RelativeLayout>